{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },

    "KeycloakOptions": {
        "AuthorizationUrl": "http://localhost:4444/realms/keycloak-auth-demo/protocol/openid-connect/auth"
    },

    "AuthenticationOptions": {
        "MetadataAddress": "http://keycloak:8080/realms/keycloak-auth-demo/.well-known/openid-configuration/",
        "ValidIssuer": "http://localhost:4444/realms/keycloak-auth-demo",
        "Audience": "account"
    },

    "AllowedHosts": "*"
}
